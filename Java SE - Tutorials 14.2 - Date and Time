import java.util.*;
import java.text.*;

public class DateDemo {

   public static void main(String args[]) {
      Date dNow = new Date( );
      SimpleDateFormat ft = 
      new SimpleDateFormat ("E dd.MM.yyyy ' \nSaat  :' hh:mm:ss a zzz");

      System.out.println("Tarih : " + ft.format(dNow));
   }
}
